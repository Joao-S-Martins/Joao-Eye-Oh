{
  "env": {
      "node": true,
      "es6": true,
      "jest": true
      "browser": true
      "esmodule": true
  },
  "extends": ["eslint:recommended", "plugin:@docusaurus/recommended", "plugin:react-hooks/recommended", "plugin:@typescript-eslint/recommended"],
  "plugins": ["@docusaurus", "react-hooks"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn", // TODO Why is this being ignored?
    "@docusaurus/string-literal-i18n-messages": "warn",
    "@docusaurus/no-untranslated-text": "warn",
    "@docusaurus/no-html-links": "warn",
    "@docusaurus/prefer-docusaurus-heading": "warn",
    "react-hooks/rules-of-hooks": "error", // Checks rules of Hooks
    "react-hooks/exhaustive-deps": "warn", // Checks effect dependencies
  }
}
